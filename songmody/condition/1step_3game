import java.io.*;
class Main {
    public static void main(String[] args) throws Exception {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        String input = br.readLine();
        int inNum = Integer.parseInt(input);

        for(int i=1; i<=inNum; i++){
            if(i % 3 == 0){
                System.out.print("X ");
            }else {
                System.out.print(i + " ");
            }
        }
    }
}